<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="310.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="onlinechat.client.controllers.AuthWindowController">
   <children>
      <TextField fx:id="loginField" layoutX="182.0" layoutY="36.0" onAction="#focusToPasswordField" prefHeight="53.0" prefWidth="383.0">
         <font>
            <Font size="30.0" />
         </font>
      </TextField>
      <Label layoutX="36.0" layoutY="36.0" prefHeight="53.0" prefWidth="120.0" text="Логин:">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label layoutX="36.0" layoutY="132.0" prefHeight="53.0" prefWidth="120.0" text="Пароль:">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordField" layoutX="182.0" layoutY="133.0" onAction="#doAuthentication" prefHeight="53.0" prefWidth="383.0" />
      <Button fx:id="enterButton" layoutX="232.0" layoutY="218.0" mnemonicParsing="false" onAction="#doAuthentication" prefHeight="53.0" prefWidth="162.0" text="Войти">
         <font>
            <Font size="30.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
