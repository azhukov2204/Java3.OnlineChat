<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="236.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="onlinechat.client.controllers.NickNameChangeController">
   <children>
      <Label layoutX="141.0" layoutY="31.0" text="Введите новое имя:">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TextField fx:id="newNickNameField" layoutX="50.0" layoutY="76.0" onAction="#changeNickName" prefHeight="65.0" prefWidth="500.0">
         <font>
            <Font size="30.0" />
         </font>
      </TextField>
      <Button layoutX="202.0" layoutY="152.0" mnemonicParsing="false" onAction="#changeNickName" text="Применить">
         <font>
            <Font size="30.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
